            <script>
                        
              now = new Date;
              var mes = now.getMonth()+1;
              var dia =now.getDate();                   

              if (mes>9) mes = '-'+mes;
              else mes = '-0'+mes;                   
              
              if (dia>9) dia ='-'+dia;
              else dia ='-0'+dia;
              
              var data =now.getFullYear()+mes+dia; 
                        
              document.getElementById("data").value= data;
            </script>

    

